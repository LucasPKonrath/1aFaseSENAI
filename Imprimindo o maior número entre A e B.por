programa {
  funcao inicio() {
    // Ler dois números A e B , e imprima o maior deles

    // Declarando Váriaveis
    real a , b

    escreva("Digite um número: ")
    leia(a)
    escreva("Digite outro número: ")
    leia(b)

    se(a > b ){
    escreva("O maior número é o ",a)
    }senao{
      escreva("O maior número é o ",b)

    }
  }
}
